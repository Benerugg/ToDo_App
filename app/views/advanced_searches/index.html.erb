<p id="notice"><%= notice %></p>

<h1>Advanced Searches</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @advanced_searches.each do |advanced_search| %>
      <tr>
        <td><%= link_to 'Show', advanced_search %></td>
        <td><%= link_to 'Edit', edit_advanced_search_path(advanced_search) %></td>
        <td><%= link_to 'Destroy', advanced_search, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Advanced Search', new_advanced_search_path %>
